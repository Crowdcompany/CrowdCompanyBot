# Telegram Bot Konfiguration
TELEGRAM_BOT_TOKEN=dein_bot_token_hier

# Sicherheit: Autorisierte Telegram User-IDs (komma-separiert)
# Beispiel: ALLOWED_USER_IDS=YOUR_CHAT_ID,1234567890
ALLOWED_USER_IDS=YOUR_CHAT_ID

# Rate Limiting: Maximale Anfragen pro Minute pro User
RATE_LIMIT_PER_MINUTE=10

# Datenpfade: Verzeichnis für Benutzerdaten (relativ zum Projektverzeichnis)
DATA_DIR=./data

# API-Keys (optional für MiniMax Proxy - aktuell nicht benötigt)
API_KEY=dein_zai_api_key_hier
